<Window x:Class="Client.Chat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Czat" Height="900" Width="673" Background="Transparent" WindowStyle="None" AllowsTransparency="True" Icon="/Image/icon.png">
    <Grid Background="Transparent">
        <Rectangle HorizontalAlignment="Left" Height="233" Margin="519,416,0,0" Stroke="Black" VerticalAlignment="Top" Width="62" RenderTransformOrigin="0.5,0.5" Grid.RowSpan="2">
            <Rectangle.Effect>
                <DropShadowEffect/>
            </Rectangle.Effect>
            <Rectangle.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="0.244"/>
                    <RotateTransform Angle="-65.601"/>
                    <TranslateTransform X="-0.248" Y="0.43"/>
                </TransformGroup>
            </Rectangle.RenderTransform>
            <Rectangle.Fill>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" >
                    <GradientStop Color="Black" Offset="0.34"/>
                    <GradientStop Color="#FFCD1FAD" Offset="1"/>
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
        <Ellipse HorizontalAlignment="Left" Height="592" Margin="10,0,0,0" VerticalAlignment="Top" Width="582" MouseDown="Ellipse_MouseDown">
            <Ellipse.Effect>
                <DropShadowEffect/>
            </Ellipse.Effect>
            <Ellipse.Fill>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" >
                    <GradientStop Color="Black" Offset="0.884"/>
                    <GradientStop Color="#FFCD1FAD" Offset="0.41"/>
                </LinearGradientBrush>
            </Ellipse.Fill>
        </Ellipse>
        <Ellipse HorizontalAlignment="Left" Height="429" Margin="184,82,0,0" Stroke="Black" VerticalAlignment="Top" Width="221" MouseDown="Ellipse_MouseDown_1" Grid.RowSpan="2">
            <Ellipse.Fill>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0.34"/>
                    <GradientStop Color="#FFCD1FAD" Offset="1"/>
                </LinearGradientBrush>
            </Ellipse.Fill>
        </Ellipse>
        <Rectangle HorizontalAlignment="Left" Height="39" Margin="318,433,0,0" Stroke="Black" VerticalAlignment="Top" Width="62" RenderTransformOrigin="0.414,-0.45" MouseDown="Rectangle_MouseDown">
            <Rectangle.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="0.954"/>
                    <RotateTransform Angle="-62.882"/>
                    <TranslateTransform X="-27.556" Y="18.842"/>
                </TransformGroup>
            </Rectangle.RenderTransform>
            <Rectangle.Fill>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" >
                    <GradientStop Color="Black" Offset="0.66"/>
                    <GradientStop Color="#FFCD1FAD"/>
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
        <Button x:Name="btnClose" Content="X" HorizontalAlignment="Left" Margin="436,52,0,0" VerticalAlignment="Top" Width="25" FontWeight="Bold" FontFamily="MS Reference Sans Serif" RenderTransformOrigin="0.5,0.5" Click="btnClose_Click">
            <Button.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3" Offset="1"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFEC9CE4"/>
                </LinearGradientBrush>
            </Button.Background>
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="37.475"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Button x:Name="btnMinimize" Content="_" HorizontalAlignment="Left" Margin="407,34,0,0" VerticalAlignment="Top" Width="25" FontFamily="MS Reference Sans Serif" RenderTransformOrigin="0.5,0.5" FontSize="14" Click="btnMinimize_Click" FontWeight="Bold">
            <Button.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3" Offset="1"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFEC9CE4"/>
                </LinearGradientBrush>
            </Button.Background>
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="22.622"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Label x:Name="LblName" Content="Qwerty" HorizontalAlignment="Left" Margin="507,517,-55,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Width="221" Background="{x:Null}" Foreground="#FFF3BAEE" FontSize="55" FontFamily="Edwardian Script ITC" FontWeight="Bold" MouseDown="LblName_MouseDown" Grid.Row="1">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="19.537"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <RichTextBox x:Name="rtxtDialogueWindow" IsReadOnly="True" AcceptsReturn="True" VerticalScrollBarVisibility="Auto" HorizontalAlignment="Left" Height="280" Margin="83,107,0,0" VerticalAlignment="Top" Width="434" BorderBrush="Black" Foreground="#FF060000" FontSize="14" FontFamily="Palatino Linotype" SelectionBrush="Black" BorderThickness="3" Background="#D8FFD6FF" AllowDrop="True" IsHitTestVisible="True" >
            <RichTextBox.Resources>
                <Style TargetType="{x:Type Paragraph}">
                    <Setter Property="Margin" Value="0" />
                    <Setter Property="Background" Value="White"/>
                </Style>
            </RichTextBox.Resources>     
        </RichTextBox>
        <TextBox x:Name="txtMessageWindow" TextWrapping="Wrap" AcceptsReturn="True" VerticalScrollBarVisibility="Auto" HorizontalAlignment="Right" Height="43" Margin="0,392,156,0" VerticalAlignment="Top" Width="434" BorderBrush="Black" Background="#D8FFD6FF" Foreground="{Binding ElementName=cmbbFontColor,Path=Text}" FontSize="{Binding Text, ElementName=cmbbFontSize}" FontFamily="{Binding ElementName=cmbbFontType,Path=Text}" SelectionBrush="Black" BorderThickness="3" TextChanged="txtMessageWindow_TextChanged"/>
        <Button x:Name="btnSend" Content="{DynamicResource Sent}" HorizontalAlignment="Left" Margin="394,442,0,0" VerticalAlignment="Top" Width="119" Height="26" FontFamily="MS Reference Sans Serif" RenderTransformOrigin="0.5,0.5" FontWeight="Bold" FontSize="14" Click="btnSend_Click" >
            <Button.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3" Offset="1"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFEC9CE4"/>
                </LinearGradientBrush>
            </Button.Background>
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.66"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Button x:Name="btnAttach" HorizontalAlignment="Left" Margin="85,441,0,0" VerticalAlignment="Top" Width="30" Height="26" Click="btnAttach_Click">
            <Image Source="Image/atach.jpg"></Image>
            <Button.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3" Offset="1"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFEC9CE4"/>
                </LinearGradientBrush>
            </Button.Background>
        </Button>
        <Button x:Name="btnEmoticons" HorizontalAlignment="Left" Margin="119,441,0,0" VerticalAlignment="Top" Width="30" Height="26" Click="Button_Click">
            <Image x:Name="imEmoticon" Source="Image/smile.png"></Image>
            <Button.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3" Offset="1"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFEC9CE4"/>
                </LinearGradientBrush>
            </Button.Background>
        </Button>
        <Button x:Name="btnTextSettings" Content="A" HorizontalAlignment="Left" Margin="153,441,0,0" VerticalAlignment="Top" Width="30" Height="26" FontWeight="Bold" FontSize="16" Foreground="#FF420534" Click="btnTextSettings_Click">
            <Button.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3" Offset="1"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFEC9CE4"/>
                </LinearGradientBrush>
            </Button.Background>
        </Button>
        <Ellipse x:Name="elFont" HorizontalAlignment="Left" Height="280" Margin="52,191,0,0" VerticalAlignment="Top" Width="480" MouseDown="Ellipse_MouseDown" Visibility="Hidden">
            <Ellipse.Effect>
                <DropShadowEffect/>
            </Ellipse.Effect>
            <Ellipse.Fill>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" >
                    <GradientStop Color="Black" Offset="0.884"/>
                    <GradientStop Color="#FFCD1FAD" Offset="0.41"/>
                </LinearGradientBrush>
            </Ellipse.Fill>
        </Ellipse>
        <Button x:Name="btnClose1" Content="X" HorizontalAlignment="Left" Margin="384,213,0,0" VerticalAlignment="Top" Width="25" FontWeight="Bold" FontFamily="MS Reference Sans Serif" RenderTransformOrigin="1.009,0.542" Click="btnClose_Click1" Visibility="Hidden">
            <Button.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3" Offset="1"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFEC9CE4"/>
                </LinearGradientBrush>
            </Button.Background>
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="16.945"/>
                    <TranslateTransform X="-0.78" Y="3.584"/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Rectangle x:Name="reFont" HorizontalAlignment="Left" Height="171" VerticalAlignment="Top" Width="366" Margin="110,242,0,0" Fill="#D8FFD6FF" Stroke="Black" StrokeThickness="3" MouseDown="Rectangle_MouseDown" Visibility="Hidden" />
        <Label x:Name="lblFontType" Content="{DynamicResource Font}" Visibility="Hidden" HorizontalAlignment="Left" Height="33" Margin="125,258,0,0" VerticalAlignment="Top" Width="110" FontWeight="Bold" FontSize="14">
            <Label.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="#FF3A0A3A" Offset="1"/>
                </LinearGradientBrush>
            </Label.Foreground>
        </Label>
        <ComboBox x:Name="cmbbFontType" ItemsSource="{x:Static Fonts.SystemFontFamilies}" SelectedIndex="0" HorizontalAlignment="Left" Visibility="Hidden" Height="24" Margin="240,258,0,0" VerticalAlignment="Top" Width="219" >
            <ComboBox.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                    <GradientStop Color="#FFE83FD7" Offset="1"/>
                </LinearGradientBrush>
            </ComboBox.Background>
        </ComboBox>
        <Label x:Name="lblFontColor" Content="{DynamicResource FontColor}" Visibility="Hidden" HorizontalAlignment="Left" Height="33" Margin="125,291,0,0" VerticalAlignment="Top" Width="110" FontWeight="Bold" FontSize="14">
            <Label.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="#FF3A0A3A" Offset="1"/>
                </LinearGradientBrush>
            </Label.Foreground>
        </Label>
        <ComboBox x:Name="cmbbFontColor" HorizontalAlignment="Left" Visibility="Hidden" Height="24" Margin="240,291,0,0" VerticalAlignment="Top" Width="219">
            <ComboBox.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                    <GradientStop Color="#FFE83FD7" Offset="1"/>
                </LinearGradientBrush>
            </ComboBox.Background>
            <ComboBoxItem Content="Black" IsSelected="True"/>
            <ComboBoxItem Content="Red" Foreground="Red"/>
            <ComboBoxItem Content="Green" Foreground="Green"/>
            <ComboBoxItem Content="Yellow" Foreground="Yellow"/>
            <ComboBoxItem Content="Blue" Foreground="Blue"/>
            <ComboBoxItem Content="Orchid" Foreground="Orchid"/>
            <ComboBoxItem Content="Orange" Foreground="Orange"/>
            <ComboBoxItem Content="Brown" Foreground="Brown"/>
            <ComboBoxItem Content="Gold" Foreground="Gold"/>
            <ComboBoxItem Content="Silver" Foreground="Silver"/>
        </ComboBox>
        <Label x:Name="lblFontSize" Content="{DynamicResource FontSize}" Visibility="Hidden" HorizontalAlignment="Left" Height="33" Margin="125,324,0,0" VerticalAlignment="Top" Width="127" FontWeight="Bold" FontSize="14">
            <Label.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="#FF3A0A3A" Offset="1"/>
                </LinearGradientBrush>
            </Label.Foreground>
        </Label>
        <ComboBox x:Name="cmbbFontSize" HorizontalAlignment="Left" Visibility="Hidden" Height="24" Margin="257,324,0,0" VerticalAlignment="Top" Width="202" >
            <ComboBox.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                    <GradientStop Color="#FFE83FD7" Offset="1"/>
                </LinearGradientBrush>
            </ComboBox.Background>
            <ComboBoxItem Content="8"/>
            <ComboBoxItem Content="9"/>
            <ComboBoxItem Content="10"/>
            <ComboBoxItem Content="11"/>
            <ComboBoxItem Content="12"/>
            <ComboBoxItem Content="14" IsSelected="True"/>
            <ComboBoxItem Content="16"/>
            <ComboBoxItem Content="18"/>
            <ComboBoxItem Content="20"/>
            <ComboBoxItem Content="22"/>
            <ComboBoxItem Content="24"/>
            <ComboBoxItem Content="26"/>
            <ComboBoxItem Content="28"/>
        </ComboBox>
        <CheckBox x:Name="chbBold" Content="B" HorizontalAlignment="Left" Visibility="Hidden" Margin="146,364,0,0" VerticalAlignment="Top" Width="27" FontWeight="Bold" FontFamily="MS Reference Sans Serif" RenderTransformOrigin="1.009,0.542" Height="26" FontSize="14" Checked="IfCheckedB" Unchecked="IfUncheckedB">
            <CheckBox.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                    <GradientStop Color="#FFE83FD7" Offset="1"/>
                </LinearGradientBrush>
            </CheckBox.Background>
        </CheckBox>
        <CheckBox x:Name="chbItalic" Content="I" HorizontalAlignment="Left" Margin="191,364,0,0" VerticalAlignment="Top" Width="27" FontWeight="Bold" FontFamily="MS Reference Sans Serif" RenderTransformOrigin="1.009,0.542" Height="26" FontSize="14" FontStyle="Italic" Visibility="Hidden" Checked="IfCheckedI" Unchecked="IfUncheckedI">
            <CheckBox.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                    <GradientStop Color="#FFE83FD7" Offset="1"/>
                </LinearGradientBrush>
            </CheckBox.Background>
        </CheckBox>
        <Ellipse x:Name="elEmoticons" HorizontalAlignment="Left" Visibility="Hidden" Height="159" Margin="37,297,0,0" VerticalAlignment="Top" Width="229" MouseDown="Ellipse_MouseDown">
            <Ellipse.Effect>
                <DropShadowEffect/>
            </Ellipse.Effect>
            <Ellipse.Fill>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" >
                    <GradientStop Color="Black" Offset="0.884"/>
                    <GradientStop Color="#FFCD1FAD" Offset="0.41"/>
                </LinearGradientBrush>
            </Ellipse.Fill>
        </Ellipse>
        <Button x:Name="btnClose2" Content="X" Visibility="Hidden" HorizontalAlignment="Left" Margin="176,302,0,0" VerticalAlignment="Top" Width="25" FontWeight="Bold" FontFamily="MS Reference Sans Serif" RenderTransformOrigin="1.009,0.542" Click="btnClose_Click2">
            <Button.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3" Offset="1"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFEC9CE4"/>
                </LinearGradientBrush>
            </Button.Background>
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="11.667"/>
                    <TranslateTransform X="-0.78" Y="3.584"/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Rectangle x:Name="reEmoticons" HorizontalAlignment="Left" Visibility="Hidden" Height="96" VerticalAlignment="Top" Width="166" Margin="69,329,0,0" Fill="#D8FFD6FF" Stroke="Black" StrokeThickness="3" MouseDown="Rectangle_MouseDown" />
        <Image x:Name="imSmile" Source="Image/smile.png" HorizontalAlignment="Left" Height="21" Margin="98,339,0,0" VerticalAlignment="Top" Width="22" RenderTransformOrigin="2.665,1.096" Visibility="Hidden"/>
        <Image x:Name="imAngel" Source="Image/angel.png" HorizontalAlignment="Left" Height="21" Margin="142,340,0,0" VerticalAlignment="Top" Width="22" Visibility="Hidden"/>
        <Image x:Name="imAshamed" Source="Image/ashamed.png" HorizontalAlignment="Left" Height="21" Margin="184,338,0,0" VerticalAlignment="Top" Width="22" Visibility="Hidden"/>
        <Image x:Name="imCry" Source="Image/crying2.png" HorizontalAlignment="Left" Height="21" Margin="101,392,0,0" VerticalAlignment="Top" Width="22" Visibility="Hidden"/>
        <Image x:Name="imHeart" Source="Image/heart.png" HorizontalAlignment="Left" Height="21" Margin="157,366,0,0" VerticalAlignment="Top" Width="22" Visibility="Hidden"/>
        <Image x:Name="imKiss" Source="Image/kiss.png" HorizontalAlignment="Left" Height="21" Margin="184,392,0,0" VerticalAlignment="Top" Width="22" Visibility="Hidden"/>
        <Image x:Name="imMad" Source="Image/mad-angry.png" HorizontalAlignment="Left" Height="21" Margin="197,364,0,0" VerticalAlignment="Top" Width="22" Visibility="Hidden"/>
        <Image x:Name="imScared" Source="Image/scared.png" HorizontalAlignment="Left" Height="21" Margin="83,364,0,0" VerticalAlignment="Top" Width="22" Visibility="Hidden"/>
        <Image x:Name="imSurprised" Source="Image/surprised.png" HorizontalAlignment="Left" Height="21" Margin="125,366,0,0" VerticalAlignment="Top" Width="22" Visibility="Hidden"/>
        <Image x:Name="imTongue" Source="Image/tongue-out.png" HorizontalAlignment="Left" Height="21" Margin="142,392,0,0" VerticalAlignment="Top" Width="22" Visibility="Hidden"/>
        <Menu HorizontalAlignment="Left" Height="26" Margin="188,441,0,0" VerticalAlignment="Top" Width="31" FontFamily="Segoe UI Symbol">
            <Menu.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3" Offset="1"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFEC9CE4"/>
                </LinearGradientBrush>
            </Menu.Background>
            <MenuItem Width="34" Height="26">
                <MenuItem.Icon>
                    <Image Source="/Image/settings.png"/>
                </MenuItem.Icon>
                <MenuItem Header="{DynamicResource ClearDialWindow}" Name="ClearDialWindow" Click="ClearDialWindow_Click">
                    <MenuItem.Icon>
                        <Image Source="/Image/clear.jpg"></Image>
                    </MenuItem.Icon>
                </MenuItem>
                <MenuItem Header="{DynamicResource ClearHistory}" Name="ClearHistory">
                    <MenuItem.Icon>
                        <Image Source="/Image/clear2.jpg"></Image>
                    </MenuItem.Icon>
                </MenuItem>
                <MenuItem Header="{DynamicResource PrintHistory}" Name="PrintHistory">
                    <MenuItem.Icon>
                        <Image Source="/Image/print.jpg"></Image>
                    </MenuItem.Icon>
                </MenuItem>
                <MenuItem Header="{DynamicResource Save}" IsCheckable="True" Name="Save">
                    <MenuItem.Icon>
                        <Image Source="/Image/save.jpg"></Image>
                    </MenuItem.Icon>
                </MenuItem>
            </MenuItem>
        </Menu>
    </Grid>
</Window>
